    "polished": "^3.4.1",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.9.0",
    "react-easy-emoji": "^1.2.0",
    "reflect-metadata": "^0.1.13",
